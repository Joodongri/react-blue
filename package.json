{
  "name": "react-blue",
  "productName": "React Blue",
  "version": "1.0.0",
  "description": "React prototyping tool for creating file structure",
  "main": "installer/main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --open",
    "build": "rm -rf ./build && cross-env NODE_ENV=production webpack",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open",
    "test": "jest --verbose",
    "dev-electron": "cross-env NODE_ENV=development electron .",
    "package-mac": "electron-packager . React\\ Blue --overwrite --platform=darwin --arch=x64 --icon=client/assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"React Blue\"",
    "package-linux": "electron-packager . react-blue --overwrite --asar=true --platform=linux --arch=x64 --icon=client/assets/icons/png/icon.png --prune=true --out=release-builds",
    "create-installer-mac": "electron-installer-dmg ./release-builds/React\\ Blue-darwin-x64/React\\ Blue.app/ React\\ Blue --out=release-builds --overwrite --icon=client/assets/icons/mac/icon.icns",
    "create-installer-win": "node installer/createInstaller.js",
    "create-installer-linux": "electron-installer-debian --src release-builds/react-blue-linux-x64/ --arch amd64 --config installer/debian.json"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "author": "team lit",
  "contributors": [
    "Darren Zhu",
    "Kendall Lu",
    "Krystal Chen",
    "Randy Reyes"
  ],
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.5.2",
    "@material-ui/icons": "^4.5.1",
    "bootstrap": "^4.3.1",
    "clone": "^2.1.2",
    "codemirror": "^5.49.0",
    "file-saver": "^2.0.2",
    "hotkeys-js": "^3.7.2",
    "jest": "^24.9.0",
    "jszip": "^3.2.2",
    "react": "^16.10.2",
    "react-animate-on-scroll": "^2.1.5",
    "react-bootstrap": "^1.0.0-beta.14",
    "react-codemirror": "^1.0.0",
    "react-d3-tree": "^1.15.3",
    "react-dom": "^16.10.2",
    "react-github-btn": "^1.1.1",
    "react-particles-js": "^2.7.0",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scroll": "^1.7.14",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/react": "^9.3.0",
    "@testing-library/user-event": "^7.1.2",
    "babel-loader": "^8.0.4",
    "concurrently": "^4.1.2",
    "cross-env": "^6.0.3",
    "css-loader": "^1.0.1",
    "electron": "^5.0.8",
    "electron-installer-dmg": "^3.0.0",
    "electron-packager": "^14.1.0",
    "electron-winstaller": "^4.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.2",
    "file-loader": "^4.0.0",
    "identity-obj-proxy": "^3.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^0.23.1",
    "url-loader": "^2.0.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-hot-middleware": "^2.24.3"
  }
}
